  <row>
    <panel>
      <input type="text" token="term" searchWhenChanged="true">
        <label>Search Term</label>
        <default>*</default>
        <initialValue>*</initialValue>
      </input>
      <table>
        <search>
          <query>index=home error* OR fail* OR critcial* OR issue* OR problem* "*$term$*"
| search sourcetype=$sourcetype$ OR process=$process$ OR services=$services$
| search _raw!="* notify_register_coalesced_registration failed with code 9 on line 2835" 
| eval Date=strftime(_time,"%d %m %Y %H:%M:%S %Z")
| table Date _raw
| sort -Date</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">10</option>
